   "source": [
    "import cv2\n",
    "\n",
    "cap = cv2.VideoCapture(0)\n",
    "\n",
    "while cap.isOpened():\n",
    "    ret, frame = cap.read()\n",
    "    img = cv2.resize(frame, (640, 480))\n",
    "    \n",
    "    cv2.imshow(\"WidowFrame\", img)\n",
    "    if cv2.waitKey(1) & 0xFF == ord(\"q\"):\n",
    "        break\n",
    "cap.release()        \n",
    "cv2.destroyAllWindows()"
   ]
